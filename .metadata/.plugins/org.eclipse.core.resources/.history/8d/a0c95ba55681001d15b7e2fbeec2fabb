package teste;

import java.io.*;

public class Teste {

	public static void main(String[] args) {
		
		String a = new String("Teste");
		String b = new String("Teste");
		
		// Testa se dois objetos são idênticos
		if (a == b) {
			System.out.println("igual");
		}
		else {
			System.out.println("diferente");
		}
		
		System.out.println("---------------------------");
		
		// Testa o conteúdo
		if (a.equals(b)) {
			System.out.println("igual");
		}
		else {
			System.out.println("diferente");
		}
	}

}
